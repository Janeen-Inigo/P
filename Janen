// Ask user for dog breed and print count
let dog_breed = prompt("Enter the dog breed:");
let print_count = parseInt(prompt("Enter how many times to print:"), 10);

// Validate if print_count is a number
if (!isNaN(print_count) && print_count > 0) {
    for (let i = 0; i < print_count; i++) {
        console.log(`${i + 1}: ${dog_breed}`);
    }
} else {
    console.log("Invalid print count. Please enter a valid number.");
}